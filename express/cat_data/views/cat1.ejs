<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Document</title>
</head>

<body>
    <a href="/cat1"> <img src="/images/cat1.jpg" alt="cat" width="300"></a>
    <ul>
        <% if (cat_one){ %>
        <li class="food">Favorite food: <%= cat_one.fav_food %> </li>
        <li class="age"> Age: <%= cat_one.age %> </li>
        <li class="spots">Sleeping spot:
            <% sleeping_spots.forEach(function(spot){ %>
            <p> <%= spot %> </p>
            <% }) %>
        </li>
        <% } %>
    </ul>
</body>

</html>